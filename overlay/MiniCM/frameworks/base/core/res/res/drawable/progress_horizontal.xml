<?xml version="1.0" encoding="UTF-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@id/background">
        <shape>
            <corners android:radius="5.0dip" />
            <gradient android:startColor="#ff9d9e9d" android:endColor="#ff747674" android:angle="270.0" android:centerY="0.75" android:centerColor="#ff5a5d5a" />
        </shape>
    </item>
    <item android:id="@id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="5.0dip" />
                <gradient android:startColor="#807dd0d6" android:endColor="#a03dacb3" android:angle="270.0" android:centerY="0.75" android:centerColor="#8055b9c0" />
            </shape>
        </clip>
    </item>
    <item android:id="@id/progress">
        <clip>
            <shape>
                <corners android:radius="5.0dip" />
                <gradient android:startColor="#ff7dd0d6" android:endColor="#ff3dacb3" android:angle="270.0" android:centerY="0.75" android:centerColor="#ff55b9c0" />
            </shape>
        </clip>
    </item>
</layer-list>